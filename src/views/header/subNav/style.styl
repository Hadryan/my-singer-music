.subnav
  width: 100%
  background-color: #C20C0C;
  height: 5px;
  font-size: 12px;
  .subnav-wrapper{
    width: 100%
    height: 34px;
    background-color: #C20C0C;
  }
  .subnav-item
    float left
    line-height: 1;
    background-color: #C20C0C;
    color: #fff;
    span 
      display: inline-block;
      height: 20px;
      padding: 0 13px;
      margin: 7px 17px 0;
      border-radius: 20px;
      line-height: 21px;
      cursor pointer
      &:hover
        background-color: #000000
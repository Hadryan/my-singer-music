.dj-wrap
  margin-top: 30px;
  font-size: 12px;
  .hd-3
    position: relative;
    height: 23px;
    margin: 0 20px;
    border-bottom: 1px solid #ccc;
    color: #333;
  .dj-ul
    margin: 20px 0 0 20px;
    &:after
      clear both
      content '.'
      height 0
      visibility hidden
      display block
    .left-wrap
      float left
      margin-right: 10px;
    .right-wrap
      float left
      .r-title
        color: #666;
        line-height: 21px;
      .r-name
        line-height: 21px;
        color: #333;
        &:hover
          text-decoration: underline;